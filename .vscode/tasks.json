    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-dbg",
            "type": "shell",
            "command": "cmake .. -DUSE_GTEST=OFF -DCMAKE_C_COMPILER=/usr/bin/gcc-11 -DCMAKE_CXX_COMPILER=/usr/bin/g++-11 -DCMAKE_BUILD_TYPE=Debug && make -j$(nproc)",
            "options": {
                "cwd": "/bango-sdk/build"
            },
            "problemMatcher": []
        }
    ]
}
